package com.cic.textengine.repository.namenode.daemon.requesthandler;

import java.net.Socket;

import com.cic.textengine.repository.namenode.daemon.NNDaemon;

public class NNRequestContext {
	NNDaemon daemon = null;
	Socket socket = null;

	public NNDaemon getDaemon() {
		return daemon;
	}

	public void setDaemon(NNDaemon daemon) {
		this.daemon = daemon;
	}

	public void setSocket(Socket socket) {
		this.socket = socket;
	}

	public Socket getSocket() {
		return socket;
	}

}
