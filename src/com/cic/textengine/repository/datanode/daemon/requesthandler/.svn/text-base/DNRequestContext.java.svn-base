package com.cic.textengine.repository.datanode.daemon.requesthandler;

import java.net.Socket;

import com.cic.textengine.repository.datanode.daemon.DNDaemon;

public class DNRequestContext {
	DNDaemon daemon = null;
	Socket socket = null;

	public DNDaemon getDaemon() {
		return daemon;
	}

	public void setDaemon(DNDaemon daemon) {
		this.daemon = daemon;
	}

	public void setSocket(Socket socket) {
		this.socket = socket;
	}

	public Socket getSocket() {
		return socket;
	}
	
}
